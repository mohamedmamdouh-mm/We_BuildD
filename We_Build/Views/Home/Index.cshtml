<!-- SERVICES -->
<section class="services" id="services">
    <h1 class="heading">@SharedLocalizer.GetLocalizedString("our services")</h1>

    <div class="box-container">
        <div class="box">
            <img src="images/service-1.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("building construction")</h3>
            <p>@SharedLocalizer.GetLocalizedString("the process of adding structure to real property via various techniques")</p>
        </div>

        <div class="box">
            <img src="images/service-2.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("house renovation")</h3>
            <p>@SharedLocalizer.GetLocalizedString("the process of repairing and improving a building so that it is in good condition again")</p>
        </div>

        <div class="box">
            <img src="images/service-3.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("architechture design")</h3>
            <p>@SharedLocalizer.GetLocalizedString("the designing and planning of structures where functionality and aesthetics are the two key elements of the process")</p>
        </div>

        <div class="box">
            <img src="images/service-4.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("material supply")</h3>
            <p>@SharedLocalizer.GetLocalizedString("Materials and supplies are deemed to include goods used as inputs in a production process")</p>
        </div>

        <div class="box">
            <img src="images/service-5.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("construction consultant")</h3>
            <p>@SharedLocalizer.GetLocalizedString("help the project owner obtain all the necessary building and trade-specific permits to complete the project")</p>
        </div>

        <div class="box">
            <img src="images/service-6.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("interior design")</h3>
            <p>@SharedLocalizer.GetLocalizedString("make indoor spaces functional, safe, and beautiful by determining space requirements and selecting essential and decorative items")</p>
        </div>

        <div class="box">
            <img src="images/service-7.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("building maintenance")</h3>
            <p>@SharedLocalizer.GetLocalizedString("regularly and repairing items that are broken It can involve inspecting repairing and maintaining electrical systems")</p>
        </div>

        <div class="box">
            <img src="images/service-8.png" alt="">
            <h3>@SharedLocalizer.GetLocalizedString("building renovation")</h3>
            <p>@SharedLocalizer.GetLocalizedString("the structural and technical restoration or modernisation of one or several floors or even an entire existing structure")</p>
        </div>
    </div>
</section>

