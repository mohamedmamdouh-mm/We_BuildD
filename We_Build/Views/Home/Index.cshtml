<!-- HOME -->
<section class="home" id="home">
    <div class="swiper home-slider">
        <div class="swiper-wrapper">
            <section class="swiper-slide slide" style="background: url(images/home-slide-2.jpg) no-repeat;">
                <div class="content">
                    <h3>@SharedLocalizer.GetLocalizedString("New Middle East General Contracting Company core values")</h3>
                    <p>@SharedLocalizer.GetLocalizedString("Excellence is the end result of knowing understanding embracing and achieving core values In the fall of 2007 site and office staff met outside to discuss core values We wanted to know what are the absolute principles that define the way New Middle East General Contracting Company operates Actually we wanted an analysis of the company's DNA")</p>
                    
                    <a href="#about" class="btn">@SharedLocalizer.GetLocalizedString("get started")</a>
                </div>
            </section>

            <section class="swiper-slide slide" style="background: url(images/home-slide-2.jpg) no-repeat;">
                <div class="content">
                    <h3>making dream come to life</h3>
                    <p>
                        Lorem, ipsum dolor sit amet consectetur adipisicing elit. Cum, odio voluptatem accusantium non
                        molestias adipisci!
                    </p>
                    <a href="#about" class="btn">get started</a>
                </div>
            </section>

            <section class="swiper-slide slide" style="background: url(images/home-slide-3.jpg) no-repeat;">
                <div class="content">
                    <h3>from concept to creation</h3>
                    <p>
                        Lorem ipsum dolor sit amet consectetur, adipisicing elit. Consequatur aperiam, doloribus eius
                        adipisci ex illum.
                    </p>
                    <a href="#about" class="btn">get started</a>
                </div>
            </section>
        </div>

        <!--<div class="swiper-button-next"></div>
        <div class="swiper-button-prev"></div>-->
    </div>
</section>

<!-- ABOUT -->
<section class="about" id="about">
    <h1 class="heading">@SharedLocalizer.GetLocalizedString("about us")</h1>

    <div class="row">
        <div class="video">
            <video src="images/about-vid.mp4" loop muted autoplay></video>
        </div>

        <div class="content">
            <h3>@SharedLocalizer.GetLocalizedString("We will provide you the best work which you dreamt for")</h3>
            <p>@SharedLocalizer.GetLocalizedString("Our sincerity reflects our honesty and integrity in our dealings with our customers and employees. Teamwork has been an integral part of our culture since the company was founded in 2005. And of course teamwork has to influence without mutual respect respect and consideration")</p>
            <a href="#services" class="btn">@SharedLocalizer.GetLocalizedString("read more")</a>
        </div>
    </div>

